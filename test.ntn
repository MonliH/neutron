get!("io/print");
get!("io/stdin");
get!("types/to_int");


is_not_win = true;
number = 10;

while (is_not_win) {
  guess = to_int(stdin(prompt="Enter A Number:"));
  if (guess == number) {
    print("You Win!");
    is_not_win = false;
  } else if (guess < number) {
    print("Too Low");
  } else if (guess > number) {
    print("Too High");
  }
}
